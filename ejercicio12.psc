Algoritmo ejercicio12
	Escribir "Inserte los valores del primer punto.";
	Escribir "Valor de x:";
	leer x1
	Escribir "Valor de y:";
	leer y1
	Escribir "Inserte los valores del segundo punto.";
	Escribir "Valor de x:";
	leer x2
	Escribir "Valor de y:";
	leer y2
	
	distancia <- raiz((x2-x1)^2+(y2-y1)^2)
	
	Escribir "La distancia entre los puntos es " distancia "unidades.";
FinAlgoritmo
